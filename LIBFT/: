//#include "libft.h"

#include <string.h>
#include <stdio.h>

int	ft_strcmp(const char *s1, const char *s2)
{
	int i;

	i = 0;
	while (s1[i] != '\0' && s2[i] != '\0')
	{
		if (s1[i] == s2[i])
			return (0);
		if (s1[i] < s2[i])
			return (-1);
		if (s1[i] > s2[i])
			return (1);
	}
	i++;
	return (s1[i] - s2[i]);
}

int	main(void)
{
	char *s1;
	char *s2;

	s1 = "hello";
	s2 = "hallo";
	printf("%d\n", ft_strcmp(s1, s2));
	return (0);
}	
